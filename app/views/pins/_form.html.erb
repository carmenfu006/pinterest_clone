<%= form_with(model: pin, id: dom_id(pin), html: { 'data-turbo-frame': '_top' }, data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset" }) do |f| %>
  <%= f.select :board_id, options_from_collection_for_select(boards, :id, :name)%>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :about %>
  <%= f.text_field :about %>
  <%= f.submit 'Save Pin' %>
<% end %>